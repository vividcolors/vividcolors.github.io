
<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-6668265-7"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-6668265-7');
  function goout(ev) {
    var url = ev.target.getAttribute("href");
    gtag('event', 'click', {
      'event_category': 'outbound',
      'event_label': url
    });
    return true;
  }
</script>

